      "source": [
        "def match(n, add_one):\n",
        "    languages = ['en', 'es', 'fr', 'in', 'it', 'nl', 'pt', 'tl']\n",
        "    data_files = {\n",
        "        'en': '/content/nlp-course/lm-languages-data-new/en.csv',\n",
        "        'es': '/content/nlp-course/lm-languages-data-new/es.csv',\n",
        "        'fr': '/content/nlp-course/lm-languages-data-new/fr.csv',\n",
        "        'in': '/content/nlp-course/lm-languages-data-new/in.csv',\n",
        "        'it': '/content/nlp-course/lm-languages-data-new/it.csv',\n",
        "        'nl': '/content/nlp-course/lm-languages-data-new/nl.csv',\n",
        "        'pt': '/content/nlp-course/lm-languages-data-new/pt.csv',\n",
        "        'tl': '/content/nlp-course/lm-languages-data-new/tl.csv'\n",
        "    }\n",
        "\n",
        "    vocabulary = list(set(chr(i) for i in range(32, 127)))\n",
        "\n",
        "    models = {}\n",
        "    for lang in languages:\n",
        "        models[lang] = lm(n, vocabulary, data_files[lang], add_one)\n",
        "\n",
        "    results = pd.DataFrame(index=languages, columns=languages)\n",
        "\n",
        "    for lang_model in languages:\n",
        "        for lang_data in languages:\n",
        "            results.at[lang_model, lang_data] = eval(n, models[lang_model], data_files[lang_data])\n",
        "\n",
        "    return results\n",
        "\n",
        "for n in range(1, 5):\n",
        "    for add_one in [False, True]:\n",
        "        print(f\"n={n}, add_one={add_one}\")\n",
        "        perplexity_df = match(n, add_one)\n",
        "        print(perplexity_df)\n",
        "        print(\"\\n\")\n"
      ],
      "execution_count": null,
